{
  "editor.accessibilitySupport": "off",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.fontFamily": "MesloLGS NF",
  "editor.fontLigatures": "true",
  "editor.fontSize": 12,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.letterSpacing": 0.5,
  "editor.matchBrackets": "always",
  "editor.minimap.showSlider": "always",
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on"
  },
  "editor.quickSuggestionsDelay": 1000000,
  "editor.renderWhitespace": "none",
  "editor.rulers": [80, 100],
  "editor.showFoldingControls": "always",
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.wordWrap": "on",
  "explorer.compactFolders": false,
  "explorer.openEditors.visible": 1,
  "files.associations": {
    "*.omnifocusjs": "javascript",
    "*.tfvars": "terraform"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.confirmForcePush": false,
  "git.openRepositoryInParentFolders": "always",
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "prettier.arrowParens": "always",
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "security.workspace.trust.enabled": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "catppuccin-mocha",
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.wordBasedSuggestions": "off",
  "editor.cursorWidth": 5,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "terminal.integrated.fontLigatures.enabled": true,
  "explorer.confirmDelete": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "line-thin",
  "vim.leader": "<space>",
  "editor.lineNumbers": "relative",
  "explorer.confirmDragAndDrop": false,
  "workbench.editor.empty.hint": "hidden",
  "git.ignoreRebaseWarning": true,
  "terminal.integrated.fontSize": 12,
  "workbench.activityBar.orientation": "vertical",
  "cursor.cpp.disabledLanguages": ["markdown", "plaintext", "yaml"],
  "vim.cursorStylePerMode.insert": "line-thin",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.replace": "block-outline",
  "vim.easymotion": true,
  "vim.targets.enable": true,
  "vim.foldfix": true,
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-s>": false,
    "<C-z>": false
  },
  "vsicons.dontShowNewVersionMessage": true,
  "window.newWindowProfile": "Default",
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[lua]": {
    "editor.defaultFormatter": "sumneko.lua"
  },
  "atlascode.bitbucket.enabled": false,
  "atlascode.rovodev.showEntitlementNotifications": false,
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "[hcl]": {
    "editor.defaultFormatter": "twiking.vscode-hcl-format"
  },
  "git.allowNoVerifyCommit": true,
  "githubPullRequests.pullBranch": "never",
  "githubPullRequests.pushBranch": "always",
  "atlascode.jira.jqlList": [
    {
      "id": "498ca5ff-b3b1-4eb1-8512-dd9cb1015117",
      "siteId": "585d17c0-ba2c-45b0-a52e-ded5f15edc33",
      "name": "My Current Work",
      "query": "assignee = currentUser()  AND issuetype != Epic  AND status NOT IN (Withdrawn, Done, Closed)  ORDER BY Rank DESC",
      "enabled": true,
      "monitor": true
    }
  ]
}
